            <div class="first-product">
                <img src="{{row2.data.image}}" class="prod-img">
                <p class="prod-txt">{{row2.data.title}}</p>
            </div>
        </a> 
        {%- endfor -%}
    </div>
    
    <div class="product-row">
        {%- for row3 in collections.row3 -%}
        <a href="{{ row3.url }}" class="navi-link">
            <div class="first-product">
                <img src="{{row3.data.image}}" class="prod-img">
                <p class="prod-txt">{{row3.data.title}}</p>
            </div>
        </a> 
        {%- endfor -%}
    </div>
</section>