<section class="all-sec">
    
    <div class="product-row">
        {%- for row1 in collections.row1 -%}
        <a href="{{ row1.url }}" class="navi-link">
            <div class="first-product">
                <img src="{{row1.data.image}}" class="prod-img">
                <p class="prod-txt">{{row1.data.title}}</p>
            </div>
        </a> 
         {%- endfor -%}
    </div>
   
    
    <div class="product-row-2">
        {%- for row2 in collections.row2 -%}
